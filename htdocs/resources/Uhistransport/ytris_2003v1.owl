    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:p1="https://riha.eesti.ee/riha/onto/keskkond.topograafia/2011/r1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1">
  <owl:Ontology rdf:about="">
    <rdfs:label xml:lang="en">Public Transport Ontology</rdfs:label>
    <rdfs:label xml:lang="et">Ühistranspordi ontoloogia</rdfs:label>
    <rdfs:comment xml:lang="et">&amp;#220;histranspordi infos&amp;#252;steemi kirjeldav ontoloogia</rdfs:comment>
    <owl:versionInfo xml:lang="et">2013/r1</owl:versionInfo>
  </owl:Ontology>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniloa_kehtetuks_tunnistamise_taotlus">
    <rdfs:label xml:lang="et">Liiniloa kehtetuks tunnistamise taotlus</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Taotlus"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">&amp;#220;histranspordiseaduse paragrahv 36 l&amp;#245;ike 9 kohane vedaja taotlus talle 
varem antud liiniloa kehtetuks tunnistamiseks eesm&amp;#228;rgiga l&amp;#245;petada liiniveo teenuse osutamine antud liinil.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Kommertsliinivedu">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liinivedu"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">&amp;#220;histranspordiseaduse paragrahv 34 l&amp;#245;ikes 6 nimetatud asutuse poolt antud liiniloa v&amp;#245;i tellijaga s&amp;#245;lmitud lepingu (v&amp;#228;lja arvatud avaliku teenindamise leping) alusel korraldatav s&amp;#245;itjatevedu.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Kommertsliinivedu</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Rong">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduk"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Rong ehk reisirong on vagunitest ja &amp;#252;hest v&amp;#245;i mitmest vedurist v&amp;#245;i 
mootorvagunist koosnev motoriseeritud veeremi&amp;#252;ksus, mis liigub m&amp;#246;&amp;#246;da r&amp;#246;&amp;#246;paid ning mida kasutatakse s&amp;#245;itjate vedamiseks. Rongina k&amp;#228;sitatakse r&amp;#246;&amp;#246;basbussi.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Rong</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Taotlus">
    <rdfs:comment xml:lang="et">Vedaja poolt &amp;#252;histranspordiseaduse paragrahv 34 l&amp;#245;ikes 6 nimetatud asutusele esitatud taotlus liiniloa andmiseks, varem antudliiniloa kehtetuks tunnistamiseks v&amp;#245;i liiniloa alusel kinnitatud s&amp;#245;iduplaani kinnitamiseks v&amp;#245;i muutmiseks.</rdfs:comment>
    <rdfs:label xml:lang="et">Taotlus</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Parvlaev">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduk"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="et">Parvlaev</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#245;itjate vedamiseks ette n&amp;#228;htud vees&amp;#245;iduk. &amp;#220;histranspordi infos&amp;#252;steemis 
h&amp;#245;lmab m&amp;#245;iste parvlaev ka s&amp;#245;itjate veoks ette n&amp;#228;htud v&amp;#228;ikelaeva.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tariif">
    <rdfs:label xml:lang="et">Tariif</rdfs:label>
    <rdfs:comment xml:lang="et">Tariif ehk s&amp;#245;idutraiif on s&amp;#245;idupileti hinna arvutamiseks kasutatav suurus, millega m&amp;#228;&amp;#228;ratakse pileti hind s&amp;#245;ltuvalt s&amp;#245;idu kaugusest v&amp;#245;i tsoonidest millesse kuuluvate peatuste vahel s&amp;#245;itja liigub.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Erijuht">
    <rdfs:comment xml:lang="et">Erijuht on seotud n&amp;#228;iteks p&amp;#252;hade ajal k&amp;#228;igus oleva s&amp;#245;iduplaaniga, mis erineb tavap&amp;#228;rasest. N&amp;#228;iteks riigip&amp;#252;hade ajal on liin k&amp;#228;igus p&amp;#252;hap&amp;#228;evase graafiku alusel, v&amp;#245;i liin on k&amp;#228;igus koolivaheaegadel kolmap&amp;#228;eval ja reedel.</rdfs:comment>
    <rdfs:label xml:lang="et">Erijuht</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaan"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uksikpilet">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidupilet"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">&amp;#220;ksikpilet on s&amp;#245;idupileti liik mis annab pileti omanikule &amp;#245;iguse s&amp;#245;ita 
teatud liinil v&amp;#245;i liinidel &amp;#252;hekordselt piletil m&amp;#228;rgitud peatuste vahel.</rdfs:comment>
    <rdfs:label xml:lang="et">Üksikpilet</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tsoonipilet">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidupilet"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">S&amp;#245;idupileti liikide klassifikatsioon vastvalt s&amp;#245;idukordade arvule, v&amp;#245;i 
s&amp;#245;itude teostamise perioodile, mille jooksul on pileti soetanud s&amp;#245;itjal &amp;#245;igus &amp;#252;histranspordivahendiga &amp;#252;hel v&amp;#245;i mitmel erineval liinil s&amp;#245;ita</rdfs:comment>
    <rdfs:label xml:lang="et">Tsoonipilet</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Kaugliin">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liin"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="et">Kaugliin</rdfs:label>
    <rdfs:comment xml:lang="et">Kaugliin on regulaarse s&amp;#245;itjateveoteenus liik, mille puhul liini 
s&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud peatused asuvad eri maakondades.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Buss">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduk"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Teeliikluses s&amp;#245;itjate vedamiseks etten&amp;#228;htud mootors&amp;#245;iduk milles on 
&amp;#252;ldjuhul lisaks juhikohale rohkem kui kaheksa istekohta ning mis kuulub 
M&lt;sub&gt;2&lt;/sub&gt;&amp;#160;ja M&lt;sub&gt;3&lt;/sub&gt;&amp;#160;kategooriasse.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Buss</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Noudepeatus">
    <rdfs:label xml:lang="et">Nõudepeatus</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Peatus"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">S&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud peatus, mis on ette n&amp;#228;htud &amp;#252;ksnes kas s&amp;#245;itjate 
    &amp;#252;hiss&amp;#245;idukisse sisenemiseks v&amp;#245;i &amp;#252;ksens s&amp;#245;itjate &amp;#252;hiss&amp;#245;idukist v&amp;#228;ljumiseks.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Troll">
    <rdfs:comment xml:lang="et">Troll ehk trollibuss on teeliikluses s&amp;#245;itjate vedamiseks ette n&amp;#228;htud 
elektrikontaktliiniga &amp;#252;hendatud r&amp;#246;&amp;#246;basteta mootors&amp;#245;iduk, milles on lisaks juhikohale rohkem kui kaheksa istekohta ning mis kuulub M&lt;sub&gt;2&lt;/sub&gt; v&amp;#245;i M&lt;sub&gt;3&lt;/sub&gt; kategooriasse.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Troll</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduk"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liin">
    <rdfs:label xml:lang="et">Liin</rdfs:label>
    <rdfs:comment xml:lang="et">Liin on s&amp;#245;iduplaanis etten&amp;#228;htud ajavahemikel ning korras avaliku teenindamise lepingu, liiniloa v&amp;#245;i tellijaga s&amp;#245;lmitud lepingu alusel vedaja &amp;#252;hiss&amp;#245;iduki l&amp;#228;bitav liikumistee.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Grupipilet">
    <rdfs:label xml:lang="et">Grupipilet</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidupilet"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Sõidupilet, millega saab korraga s&amp;#245;ita mitu inimest.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Veoots">
    <rdfs:label xml:lang="et">Veoots</rdfs:label>
    <rdfs:comment xml:lang="et">Veoots (e. v&amp;#228;ljumine, e. reis) on s&amp;#245;iduki liini &amp;#252;ks terviklik s&amp;#245;iduts&amp;#252;kkel 
    algpunktist (peatusest) l&amp;#245;pp-punkti (peatusse) v&amp;#245;i l&amp;#245;pp-punktist 
    (peatusest) algpunkti (peatusse).</rdfs:comment>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaan"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Euroopa_Uhenduse_tegevusluba">
    <rdfs:comment xml:lang="et">Euroopa Parlamendi ja n&amp;#245;ukogu m&amp;#228;&amp;#228;ruse (E&amp;#220;) nr 1073/2009 artikli 4 kohane 
dokument, mis t&amp;#245;endab selle omaja &amp;#245;igust korraldada tasu eest riigisisest ja rahvusvahelist liinivedu liiniloa v&amp;#245;i lepingu alusel ning riigisisest ja rahvusvahelist juhuvedu.</rdfs:comment>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Vedaja"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniluba"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="et">Euroopa &amp;#220;henduse tegevusluba</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Perioodipilet">
    <rdfs:label xml:lang="et">Perioodipilet</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidupilet"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Perioodipilet on s&amp;#245;idupileti liik, mis annab pileti omanikule &amp;#245;iguse s&amp;#245;ita kindlal liinil v&amp;#245;i liinidel piletiga m&amp;#228;&amp;#228;ratud aja jooksul piiramata seejuures s&amp;#245;idukordade arvu. (n&amp;#228;iteks tunnipilet, p&amp;#228;evapilet, n&amp;#228;dalapilet, kuupilet, 30 p&amp;#228;eva pilet jne)</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaan">
    <rdfs:label xml:lang="et">Sõiduplaan</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#245;iduplaan on dokument, millega m&amp;#228;&amp;#228;ratakse &amp;#252;hiss&amp;#245;iduki liikumistee, v&amp;#228;ljumisajad (kaugliinidel ka saabumisajad) ning peatused s&amp;#245;itjate sisenemiseks ja v&amp;#228;ljumiseks;</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Avaliku_teenindamise_leping">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liinivedu"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Vedaja ja riigi v&amp;#245;i kohaliku omavalitsuse (ehk tellija) vahel s&amp;#245;lmitud 
leping, millega vedajale pannakse kohustus osutada s&amp;#245;itjateveo teenust kindlas piirkonnas leping s&amp;#228;testatud liinidel, mahus ning tingimustel.</rdfs:comment>
    <rdfs:label xml:lang="et">Avaliku teenindamise leping</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Algpeatus">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Peatus"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Liini s&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud veootsa ehk v&amp;#228;ljumise esimene peatus, mis 
on ette n&amp;#228;htud s&amp;#245;itjate &amp;#252;hiss&amp;#245;idukisse sisenemiseks.</rdfs:comment>
    <rdfs:label xml:lang="et">Algpeatus</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tsooni_piiriuletustariif">
    <rdfs:comment xml:lang="et">Tsoonipileti hinnale lisanduv tariif, mis tuleb maksta tsoonipiiri &amp;#252;letamise eest. Piiri&amp;#252;letustariif v&amp;#245;ib s&amp;#245;ltuda &amp;#252;letatavate tsoonipiiride arvust, selliselt et igas j&amp;#228;rgmises tsoonis s&amp;#245;itmine lisab piletihinnale v&amp;#228;ikema piiri&amp;#252;letustariifi.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tariif"/>
    <rdfs:label xml:lang="et">Tsooni piiriületustariif</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Peatus">
    <rdfs:label xml:lang="et">Peatus</rdfs:label>
    <rdfs:comment xml:lang="et">&amp;#220;histranspordi infos&amp;#252;steemi andmekogusesse kantud asukohas paiknev 
    rajatis, mis on ette n&amp;#228;htud s&amp;#245;itjate &amp;#252;hiss&amp;#245;idukisse sisenemiseks ja/v&amp;#245;i 
    sellest v&amp;#228;ljumiseks.
Liiklusm&amp;#228;rgiga 541 t&amp;#228;histatud koht &amp;#252;histranspordi liinil. M&amp;#245;iste h&amp;#245;lmab ka 
    liiklusm&amp;#228;rgiga t&amp;#228;histamata rongi peatusi/ jaamasid ja parvlaevaliikluse sadamaid.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Rahvusvaheline_kaugliin">
    <rdfs:label xml:lang="et">Rahvusvaheline kaugliin</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liin"/>
    <rdfs:comment xml:lang="et">Rahvusvaheline kaugliin on teeliikluses teostatava regulaarse 
s&amp;#245;itjateveoteenuse liik, mille puhul liini s&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud peatused asuvad eri riikides.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Eriotstarbeline_liinivedu">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Eriotstarbeline liinivedu</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liinivedu"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Eriotstarbelise liiniveona k&amp;#228;sitatakse j&amp;#228;rgmisi vedusid: &amp;#8211; t&amp;#246;&amp;#246;liste vedu 
t&amp;#246;&amp;#246;le ja tagasi; &amp;#8211; &amp;#245;pilaste ja &amp;#252;li&amp;#245;pilaste vedu &amp;#245;ppeasutusse ja tagasi; &amp;#8211; s&amp;#245;jav&amp;#228;elaste ja nende perekondade vedu ajutisse paiknemiskohta ja tagasi. Eriotstarbelise liiniveo v&amp;#245;ib korraldada avaliku teenindamise 
lepingu alusel v&amp;#245;i kommertsalusel.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Hinnakujunduse_piirkond">
    <rdfs:label xml:lang="et">Hinnakujunduse piirkond</rdfs:label>
    <rdfs:comment xml:lang="et">Geograafiline piirkond, mille piires opereerivatel liinidel toimub piletihinna arvutamine &amp;#252;hesuguste hinnaparameetrite (valemi) alusel. Hinnakujunduse piirkonnas on &amp;#252;ldjuhul ketestatud &amp;#252;he s&amp;#245;idukilomeetri tariif v&amp;#245;i jaotatud hinnakujunduse piirkond tsoonideks, mille vahel ja mille siseselt s&amp;#245;itmine on hinnastatud &amp;#252;hesuguste hinnaparameetrite alusel. &amp;#220;ldjuhul kehtib hinnakujunduse piirkonna siseselt &amp;#252;hesugused hinnaparameetrid ainult avalikel liinidel.</rdfs:comment>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Pileti_hind"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liinivedu">
    <rdfs:label xml:lang="et">Liinivedu</rdfs:label>
    <rdfs:comment xml:lang="et">Kindlal liikumisteel ning vastavalt s&amp;#245;iduplaanile osutatav tasuline s&amp;#245;itjateveo teenus, mis on oma iseloomult regulaarne ja stabiilne.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Vedaja">
    <rdfs:label xml:lang="et">Vedaja</rdfs:label>
    <rdfs:comment xml:lang="et">&amp;#220;histransporditeenuse osutamiseks s&amp;#245;itjateveo tegevusluba omav ettev&amp;#245;tja</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Lisaveoots">
    <rdfs:label xml:lang="et">Lisaveoots</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#245;iduplaaniv&amp;#228;line, kuid Tellija soovil v&amp;#245;i koosk&amp;#245;lastatult Tellijaga teostatav veoots - ehk peatustest v&amp;#228;ljumise/saabumise ajad on eelnevalt teada.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaan"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Hinnatabel">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Pileti_hind"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">Hinnatabel ehk hinnamaatriks on liini marsruudi peatuste vaheliste 
s&amp;#245;idupileti hindade s&amp;#252;stemaatiliselt korrastatud kogum, kus &amp;#252;hel teljel on esitatud s&amp;#245;idu algpeatus ja teisel teljel s&amp;#245;idu sihtpeatus ning 
telgede ristumiskohal on toodud nende peatuse vahelise s&amp;#245;itmiseks 
kehtetstud piletihind.</rdfs:comment>
    <rdfs:label xml:lang="et">Hinnatabel</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Lennuk">
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduk"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="et">Lennuk</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#245;itjate vedamiseks ette n&amp;#228;htud &amp;#245;hus&amp;#245;iduk.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tramm">
    <rdfs:label xml:lang="et">Tramm</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#245;itjate vedamiseks etten&amp;#228;htud elektrikontaktliiniga &amp;#252;hendatud r&amp;#246;&amp;#246;bastel 
liikuv s&amp;#245;iduk, mida kasutatakse peamiselt linnaliinidel.</rdfs:comment>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduk"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidusoodustus">
    <rdfs:label xml:lang="et">Sõidusoodustus</rdfs:label>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Pileti_hind"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="et">S&amp;#245;idusoodustus on teatava kategooria s&amp;#245;itjale seadusega v&amp;#245;i seaduse 
alusel kehtestatud korras antud &amp;#245;igus: &amp;#160;1)&amp;#160;s&amp;#245;ita &amp;#252;hiss&amp;#245;idukis tasuta v&amp;#245;i osalise tasu eest; &amp;#160;2)&amp;#160;s&amp;#245;ita &amp;#252;hiss&amp;#245;idukis t&amp;#228;ishinna eest ning saada enne v&amp;#245;i hiljem osalist v&amp;#245;i t&amp;#228;ielikku rahalist h&amp;#252;vitist.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduk">
    <rdfs:label xml:lang="et">Ühissõiduk</rdfs:label>
    <rdfs:comment xml:lang="et">&amp;#220;histransporditeenuse osutamiseks etten&amp;#228;htud buss, tramm, trollibuss, reisirong, reisiparvlaev, v&amp;#228;ikelaev, &amp;#245;hus&amp;#245;iduk v&amp;#245;i muu mootori j&amp;#245;ul liikuv transpordivahend.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Lopp-peatus">
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Peatus"/>
    <rdfs:label xml:lang="et">Lõpp-peatus</rdfs:label>
    <rdfs:comment xml:lang="et">Liini s&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud veootsa ehk v&amp;#228;ljumise viimane peatus, mis 
on ette n&amp;#228;htud s&amp;#245;itjate &amp;#252;hiss&amp;#245;idukist v&amp;#228;ljumiseks.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniloa_andmise_taotlus">
    <rdfs:label xml:lang="et">Liiniloa andmise taotlus</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Taotlus"/>
    <rdfs:comment xml:lang="et">Taotlus kommertsliiniveo &amp;#245;igust t&amp;#245;endava dokumendi, liiniloa, 
esmakordseks andmiseks v&amp;#245;i korduvaks andmiseks seoses varem antud dokumendi kehtetuks muuutumisega.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhistranspordiluba">
    <rdfs:label xml:lang="et">Ühistranspordiluba</rdfs:label>
    <rdfs:comment xml:lang="et">Tasulise s&amp;#245;itjateveo &amp;#245;igust t&amp;#245;endav dokument, mida anti vedajatele kuni 
    31.12.2011 kehtinud &amp;#252;histranspordiseaduse&amp;#160;redaktsiooni&amp;#160;&amp;#167;31 kohaselt. 
Vastvalt &amp;#252;histranspordiseaduse rakendus&amp;#228;stetele kehtib enne 2012. aasta 1. 
    jaanuari vedajale kehtinud n&amp;#245;uete kohaselt antud &amp;#252;histranspordiluba, loal 
    v&amp;#245;i s&amp;#245;idukikaardil m&amp;#228;rgitud ajani. 
&amp;#220;histranspordiluba t&amp;#245;endab vedaja &amp;#245;igust: &amp;#160;1)&amp;#160;korraldada riigisisest juhuvedu; 
    &amp;#160;2)&amp;#160;taotleda &amp;#245;igust korraldada liinivedu liiniloa v&amp;#245;i lepingu alusel; 
    &amp;#160;3)&amp;#160;&amp;#160;taotleda &amp;#245;igust korraldada rahvusvahelist vedu.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Vedaja"/>
    <rdfs:subClassOf>
      <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniluba"/>
    </rdfs:subClassOf>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Vahepeatus">
    <rdfs:comment xml:lang="et">S&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud peatus, mis ei ole algpeatus ega l&amp;#245;pp-peatus ning 
mis on ette n&amp;#228;htud nii s&amp;#245;itjate &amp;#252;hiss&amp;#245;idukisse sisenemiseks kui ka &amp;#252;hiss&amp;#245;idukist v&amp;#228;ljumiseks.</rdfs:comment>
    <rdfs:label xml:lang="et">Vahepeatus</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Peatus"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Maakonnaliin">
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liin"/>
    <rdfs:label xml:lang="et">Maakonnaliin</rdfs:label>
    <rdfs:comment xml:lang="et">Maakonnaliin&amp;#160;on regulaarse s&amp;#245;itjateveoteenus liik, mille puhul liini 
s&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud peatused asuvad &amp;#252;he maakonna piires, kuid suures osas erinevate omavalitsus&amp;#252;ksuse territooriumil.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Avalik_liinivedu">
    <rdfs:comment xml:lang="et">Liinivedu, mida teostatakse vedaja ja riigi v&amp;#245;i omavalitsus&amp;#252;ksuse vahel 
s&amp;#245;lmitud lepingu ehk avaliku teenindamise lepingu (ATL) alusel. Avaliku liiniveo teostamise tingimused ja mahu m&amp;#228;&amp;#228;rab liiniveo tellija, kelleks 
on riik v&amp;#245;i kohalik omavalitsus.</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Avalik liinivedu</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liinivedu"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Piirkonna_hinnaparameetrid">
    <rdfs:comment xml:lang="et">Piletihinna arvutamiseks kasutatavad parameetrid, mille alusel on 
    kalkuleeritakse &amp;#252;hes hinnakujunduse piirkonnas kehtiv s&amp;#245;idupileti hind 
    vastvalt s&amp;#245;idu kaugusele v&amp;#245;i l&amp;#228;bitavate tsoonide arvule.
Hinnakujunduse piirkonda iseloomustavate parameetrite v&amp;#228;&amp;#228;rtused l&amp;#228;bi ajaloo. 
    Parameetriteks on piirkonna minimaalne ja maksimaalne piletihind, Maavanema poolt kehtestatud maksimumtariif, piirkonnas kasutatav tariifi liik jne.</rdfs:comment>
    <rdfs:label xml:lang="et">Piirkonna hinnaparameetrid</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Hinnakujunduse_piirkond"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidupilet">
    <rdfs:label xml:lang="et">Sõidupilet</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#252;idupilet on paberkandjal v&amp;#245;i muul kujul, sealhulgas elektroonilisel andmekandjal, dokument, mis t&amp;#245;endab s&amp;#245;itja s&amp;#245;idu&amp;#245;igust.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Korrapilet">
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidupilet"/>
    <rdfs:label xml:lang="et">Korrapilet</rdfs:label>
    <rdfs:comment xml:lang="et">Korrapilet on s&amp;#245;idupileti liik, mis annab pileti omanikule &amp;#245;iguse s&amp;#245;ita kindlal liinil v&amp;#245;i liinidel piletiga m&amp;#228;&amp;#228;ratud arvu kordi. (N&amp;#228;iteks viie, k&amp;#252;mne jne s&amp;#245;idukorra pilet)</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Linnaliin">
    <rdfs:comment xml:lang="et">Linnaliin&amp;#160;on regulaarse s&amp;#245;itjateveoteenus liik (sh. s&amp;#245;itjatevedu trolli 
ja trammiga), mille puhul liini s&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud peatused asuvad peamiselt &amp;#252;he linna haldusterritooriumil ning mis on kujundatud l&amp;#228;htudes vastava linna elanike liikumisvajadusest.</rdfs:comment>
    <rdfs:label xml:lang="et">Linnaliin</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liin"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Vallaliin">
    <rdfs:label xml:lang="et">Vallaliin</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liin"/>
    <rdfs:comment xml:lang="et">Vallaliin&amp;#160;on regulaarse s&amp;#245;itjateveoteenuse liik, mille puhul liini 
s&amp;#245;iduplaaniga m&amp;#228;&amp;#228;ratud peatused asuvad peamiselt &amp;#252;he valla haldusterritooriumil ning mis on kujundatud l&amp;#228;htudes vastava valla 
elanike liikumisvajadusest.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Edasi-tagasi_pilet">
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Korrapilet"/>
    <rdfs:label xml:lang="et">Edasi-tagasi pilet</rdfs:label>
    <rdfs:comment xml:lang="et">Samade peatuste v&amp;#245;i tsoonide vahel (erinevates suundades) kehtiv mitme (2) korra pilet</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaani_kinnitamise_taotlus">
    <rdfs:label xml:lang="et">Sõiduplaani kinnitamise taotlus</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Taotlus"/>
    <rdfs:comment xml:lang="et">Taotlus kehtiva liiniloa alusel teenindatava liini s&amp;#245;iduplaani 
muutmiseks l&amp;#228;bi uue s&amp;#245;iduplaani kinnitamise. Esmakordselt kinnitatakse liiniloa alusel teenindatava liini s&amp;#245;iduplaan liiniloa andmisel.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Pileti_hind">
    <rdfs:comment xml:lang="et">S&amp;#245;idu&amp;#245;iguse lunastamise eest makstav rahalises v&amp;#228;&amp;#228;ringus tasu.</rdfs:comment>
    <rdfs:label xml:lang="et">Pileti hind</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tsooni_baastariif">
    <rdfs:comment xml:lang="et">Tsoonipileti hinda m&amp;#228;&amp;#228;rav baashind ehk miinimumhind, mille eest saab 
s&amp;#245;ita &amp;#252;he tsooni piires.</rdfs:comment>
    <rdfs:label xml:lang="et">Tsooni baastariif</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tariif"/>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Marsruut">
    <rdfs:label xml:lang="et">Marsruut</rdfs:label>
    <rdfs:comment xml:lang="et">Marsruut on liini teenindava &amp;#252;hiss&amp;#245;iduki liikumistee kirjeldus tema 
liikumisteel olevate peatuste ning nende vahekauguste alusel.</rdfs:comment>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniluba">
    <rdfs:comment xml:lang="et">Liiniluba t&amp;#245;endab vedaja &amp;#245;igust korraldada kommertsliinivedu.</rdfs:comment>
    <rdfs:label xml:lang="et">Liiniluba</rdfs:label>
  </rdfs:Class>
  <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Eriotstarbelise_kommertsliiniveo_leping">
    <rdfs:label xml:lang="et">Eriotstarbelise kommertsliiniveo leping</rdfs:label>
    <rdfs:comment xml:lang="et">Vedaja ja tellija s&amp;#245;lmitud eriotstarbelise kommertsliiniveo leping t&amp;#245;endab vedaja &amp;#245;igust korraldada eriotstarbelist kommertsliinivedu &amp;#252;hel v&amp;#245;i mitmel 
    liinil.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liinivedu"/>
  </rdfs:Class>
  <owl:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Piletimuugisusteem">
    <rdfs:label xml:lang="et">Piletimüügisüsteem</rdfs:label>
    <rdfs:comment xml:lang="et">Tehnikate kogum s&amp;#245;idutasu kogumiseks ja juurdep&amp;#228;&amp;#228;su kontrollimiseks.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduki_mahutavus">
    <rdfs:comment xml:lang="et">Maksimaalne s&amp;#245;itjate arv, mille vedamiseks on &amp;#252;hiss&amp;#245;iduk tootja poolt 
konstrueeritud ja ette n&amp;#228;htud.</rdfs:comment>
    <rdfs:label xml:lang="et">Ühissõiduki mahutavus</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Jalgitav_soiduk">
    <rdfs:label xml:lang="et">Jälgitav sõiduk</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#245;iduk, mis on varustatud reaalajas andmeid edastava j&amp;#228;lgimisseadmega ning mille kohta kajastatakse registris tema staatust kirjeldavat infot.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidugraafik">
    <rdfs:comment xml:lang="et">Liinile planeeritud k&amp;#245;ikide s&amp;#245;idukite (j&amp;#228;rjekordade) intervalli ning ekspluatatsiooni- ja nulls&amp;#245;iduaega arvestav t&amp;#246;&amp;#246;re&amp;#382;iim.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaan"/>
    <rdfs:label xml:lang="et">Sõidugraafik</rdfs:label>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Hinnatsoon">
    <rdfs:comment xml:lang="et">&amp;#220;he hinnakujunduse piirkonna sisene ala, milles paiknevate peatuste vahelisel s&amp;#245;itmisel kehtib &amp;#252;hesugune piletihind. Samuti on defineeritud erinevate tsoonide vahelise s&amp;#245;itmise piletihind. Tsoonide defineerimise eesm&amp;#228;rk on v&amp;#228;hendada piletihinna arvutamisel esinevate erinevate alg- ja l&amp;#245;pppeatuste kombinatsioonide arvu.</rdfs:comment>
    <rdfs:range rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Piirkonna_hinnaparameetrid"/>
    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Hinnakujunduse_piirkond"/>
    <rdfs:label xml:lang="et">Hinnatsoon</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Esitatavad_dokumendid">
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaani_kinnitamise_taotlus"/>
          <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniloa_kehtetuks_tunnistamise_taotlus"/>
          <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniloa_andmise_taotlus"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liiniluba"/>
    <rdfs:comment xml:lang="et">&amp;#220;histranspordiseaduse paragrahv 39 l&amp;#245;ikes 4 s&amp;#228;testatud dokumendid, mis vedaja peab liiniloa andjale koos liiniloa andmise taotluse v&amp;#245;i s&amp;#245;iduplaani kinnitamise taotlusega esitama.</rdfs:comment>
