    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Marsruut"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="et">Marsruudi geoobjekt</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Seisukohtade_arv">
    <rdfs:comment xml:lang="et">Maksimaalne seisvate s&amp;#245;itjate arv, keda on lubatud lisaks istuvatele s&amp;#245;itjatele &amp;#252;hiss&amp;#245;idukis vedada.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:label xml:lang="et">Seisukohtade arv</rdfs:label>
    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Uhissoiduki_mahutavus"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soodustuse_maar">
    <rdfs:comment xml:lang="et">V&amp;#228;&amp;#228;rtus, mille v&amp;#245;rra madala tasu eest on soodustatud isikul &amp;#245;igus s&amp;#245;ita &amp;#252;hiss&amp;#245;idukis. Soodustuse m&amp;#228;&amp;#228;r v&amp;#245;ib olla v&amp;#228;ljendatud protsentides s&amp;#245;idupileti t&amp;#228;ishinnast v&amp;#245;i kindla arvv&amp;#228;&amp;#228;rtusena, mis lahutatakse t&amp;#228;ispileti hinnast maha.</rdfs:comment>
    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidusoodustus"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="et">Soodustuse määr</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soodustuse_andja">
    <rdfs:label xml:lang="et">Soodustuse andja</rdfs:label>
