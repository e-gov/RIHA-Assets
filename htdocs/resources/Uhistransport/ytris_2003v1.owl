    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Jarjekord">
    <rdfs:comment xml:lang="et">J&amp;#228;rjekord on liini erinevate veootsade ehk v&amp;#228;ljumiste teenindamise ajalise j&amp;#228;rgnevuse kirjeldus. J&amp;#228;rjekord vastab &amp;#252;he s&amp;#245;iduki s&amp;#245;idugraafikuj&amp;#228;rgsele p&amp;#228;evasele t&amp;#246;&amp;#246;korraldusele liinil. Erandkorras v&amp;#245;ib &amp;#252;ks j&amp;#228;rjekord h&amp;#245;lmata erinevate liinide ajaliselt teineteisele j&amp;#228;rgnevate veootsasid.</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Liin"/>
          <owl:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidugraafik"/>
          <rdfs:Class rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soiduplaan"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label xml:lang="et">Järjekord</rdfs:label>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Baashind">
