    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Tariif"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soodustuse_piirkond">
    <rdfs:label xml:lang="et">Soodustuse piirkond</rdfs:label>
    <rdfs:comment xml:lang="et">Haldus&amp;#252;ksuste alusel territoriaalselt piiritletud ala, mille piires 
teenindatavate avalikel liinidel vaadeldav soodustus kehtib. N&amp;#228;iteks v&amp;#245;ib vastvalt &amp;#252;histranspordiseaduse paragrahvile 29 kehtestada s&amp;#245;idusoodustuse &amp;#252;he maakonna piires teenindatavatele avalikele bussiliinidele.</rdfs:comment>
    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Soidusoodustus"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Marsruudi_geoobjekt">
