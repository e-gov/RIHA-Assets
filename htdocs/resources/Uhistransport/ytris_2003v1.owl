  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Miinimumhind">
    <rdfs:domain rdf:resource="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Piirkonna_hinnaparameetrid"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:label xml:lang="et">Miinimumhind</rdfs:label>
    <rdfs:comment xml:lang="et">S&amp;#245;idupileti madalaim hind, mis kehtib juhul, kui s&amp;#245;idu kauguse ja kaugustariifi alusel arvutatuna kujuneks s&amp;#245;idupileti hind v&amp;#228;iksemaks kui miinimumhind. Tsoonitariifiga hinnakujundus korral vastab miinimumhinnale tsooni baastariif.</rdfs:comment>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://riha.eesti.ee/riha/onto/transport.yhistransport/2013/r1/Pileti_liik">
